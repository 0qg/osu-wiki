{
  "plugins": [
    "preset-lint-markdown-style-guide",
    ["remark-frontmatter", {"type": "yaml", "marker": "-"}],
    ["remark-lint-maximum-line-length", false],
    ["remark-lint-maximum-heading-length", false],
    ["remark-lint-list-item-spacing", {"checkBlanks": false}],
    ["remark-lint-no-missing-blank-lines", {"exceptTightLists": true}],
    ["remark-lint-ordered-list-marker-value", "ordered"],
    ["remark-lint-table-pipe-alignment", false],
    ["remark-lint-definition-case", false],
    ["remark-lint-final-newline"],
    ["remark-lint-no-heading-indent"],
    ["remark-lint-no-tabs"],
    ["remark-lint-no-emphasis-as-heading", false],
    ["remark-lint-list-item-indent", "space"],
    ["remark-lint-no-duplicate-headings", false],
    ["remark-lint-no-duplicate-headings-in-section"],
    ["remark-lint-final-definition", false],
    ["remark-lint-no-shortcut-reference-link", false],
    ["remark-lint-fenced-code-flag", {"allowEmpty": true}],
    ["remark-lint-no-file-name-irregular-characters", "\\.a-zA-Z0-9-_!"],
    ["remark-lint-no-file-name-mixed-case", false],
    ["remark-lint-no-heading-like-paragraph"],
    ["remark-lint-no-duplicate-definitions"],
    ["remark-lint-no-file-name-articles", false],
    ["remark-lint-no-file-name-consecutive-dashes", false],
    ["remark-lint-name-outer-dashes", false],
    ["remark-lint-no-heading-content-indent"],
    ["remark-lint-no-undefined-references"],
    ["remark-lint-no-unused-definitions"]
  ]
}
